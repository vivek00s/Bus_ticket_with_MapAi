        /* Reset default styles */
        body, ul {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
        }

        /* Fixed Navbar */
        .navbar {
            position: fixed;
            top: 0;
            width: 100%;
            background: #2c3e50;
            color: white;
            display: flex;
            justify-content: space-between;
            align-items: center;
            padding: 15px 30px;
            z-index: 1000;
            box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
            height:100px;
        }

        .navbar .logo {
            font-size: 1.8em;
            font-weight: bold;
            margin-left:20px;
        }

        .navbar ul {
            list-style: none;
            display: flex;
            gap: 20px;
        }

        .navbar ul li {
            display: inline;
        }

        .navbar ul li a {
            color: white;
            text-decoration: none;
            font-weight: bold;
            padding: 8px 15px;
            transition: 0.3s ease;
            border-radius: 5px;
        }

        .navbar ul li a:hover {
            background: #34495e;
        }

        /* User Authentication Links */
        .user-auth a {
            background: #e74c3c;
            color: white;
            padding: 8px 12px;
            border-radius: 5px;
            text-decoration: none;
            font-size: 1em;
            transition: 0.3s;
        }

        .user-auth a:hover {
            background: #c0392b;
        }

        /* Search Bar */
        .templatemo_right_section_content {
            display: flex;
            align-items: center;
            background: white;
            padding: 8px 15px;
            border-radius: 25px;
            box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
            margin-right: 80px;
        }

        .templatemo_right_section_content input[type="text"] {
            border: none;
            padding: 8px;
            font-size: 16px;
            outline: none;
            width: 250px;
        }

        .templatemo_right_section_content input[type="submit"] {
            background: #f39c12;
            border: none;
            padding: 8px 15px;
            border-radius: 20px;
            font-size: 16px;
            color: white;
            cursor: pointer;
            transition: 0.3s;
        }

        .templatemo_right_section_content input[type="submit"]:hover {
            background: #e67e22;
        }

        /* Page Content */
        main {
            margin-top: 80px;
            padding: 20px;
        }

        .alert {
            padding: 10px;
            border-radius: 5px;
            margin: 10px 0;
        }

        .alert-success {
            background: #2ecc71;
            color: white;
        }

        .alert-danger {
            background: #e74c3c;
            color: white;
        }

        .navbar {
    background: #2c3e50; /* Dark background */
    padding: 15px 0;
    position: fixed; /* Fix navbar at the top */
    width: 100%;
    top: 0;
    left: 0;
    z-index: 1000;
    display: flex;
    justify-content: center;
}

.navbar ul {
    list-style: none;
    display: flex;
    gap: 15px;
    padding: 0;
